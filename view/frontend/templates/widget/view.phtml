<script>
 require([
  'jquery',
  'matchMedia',
  'slick'
 ], function($, mediaCheck, slick) {

   var $slick_slider = $('#hellloooo03');
   $slick_slider.slick({ "mobileFirst": true, "dots": true, "infinite": false, "speed": 300, "slidesToShow": 1, "slidesToScroll": 1 });


   mediaCheck({
    media: '(max-width: 699px)',
    entry: function() {
     
     if($slick_slider.hasClass('slick-initialized'))
      $slick_slider.slick('unslick');
     return
    },
    exit: function() {
     if(!$slick_slider.hasClass('slick-initialized'))
      return $slick_slider.slick("resize");;
     }
   });



 });
</script>

